issn = {0004637X},
journal = {The Astrophysical Journal},
keywords = {GAMMA RAYS: BURSTS,Gamma Rays: Bursts,STARS: BINARIES: CLOSE,STARS: NEUTRON,STARS: SUPERNOVAE: GENERAL,Stars: Binaries: Close,Stars: Neutron,Stars: Supernovae: General},
month = {nov},
number = {1},
pages = {L59--L62},
primaryClass = {astro-ph},
title = {{Transient Events from Neutron Star Mergers}},
url = {http://arxiv.org/abs/astro-ph/9807272 http://dx.doi.org/10.1086/311680 http://arxiv.org/abs/astro-ph/9807272{\%}0Ahttp://dx.doi.org/10.1086/311680 http://stacks.iop.org/1538-4357/507/i=1/a=L59},
volume = {507},
